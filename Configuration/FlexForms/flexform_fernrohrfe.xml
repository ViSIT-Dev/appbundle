<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
<ROOT type="array">
    <type>array</type>
    <el type="array">
        <imprint>
            <TCEforms type="array">
                <config type="array">
                    <type>text</type>
                    <cols>48</cols>
                    <rows>5</rows>
                </config>
                <label>Impressum</label>
                <defaultExtras>richtext[*]:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
            </TCEforms>
        </imprint>
        <encoderOffsetX>
            <TCEforms type="array">
                <label>Encoder Offset X (RawX)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                        <upper>65536</upper>
                    </range>
                </config>
            </TCEforms>
        </encoderOffsetX>
        <encoderOffsetY>
            <TCEforms type="array">
                <label>Encoder Offset Y (RawY)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                      <lower>0</lower>
                        <upper>65536</upper>
                    </range>
                </config>
            </TCEforms>
        </encoderOffsetY>
       <xstart>
            <TCEforms type="array">
                <label>App Start X (CurrentX)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                </config>
            </TCEforms>
        </xstart>
        <ystart>
            <TCEforms type="array">
                <label>App Start Y (CurrentY)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                </config>
            </TCEforms>
        </ystart>
        <xend>
            <TCEforms type="array">
                <label>App End X (CurrentX)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                </config>
            </TCEforms>
        </xend>
        <yend>
            <TCEforms type="array">
                <label>App End Y (CurrentY)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                </config>
            </TCEforms>
        </yend>
        <navIconPosX>
            <TCEforms type="array">
                <label>Nav Icon Pos X (CurrentY)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                </config>
            </TCEforms>
        </navIconPosX>
        <navIconPosY>
            <TCEforms type="array">
                <label>Nav Icon Pos Y (CurrentY)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                </config>
            </TCEforms>
        </navIconPosY>
        <boardSizeX>
            <TCEforms type="array">
                <label>Board Size X (px)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                    <default>8000</default>
                </config>
            </TCEforms>
        </boardSizeX>
        <boardSizeY>
            <TCEforms type="array">
                <label>Board Size Y (px)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                        <lower>0</lower>
                    </range>
                    <default>4200</default>
                </config>
            </TCEforms>
        </boardSizeY>
        <sensorhost>
            <TCEforms type="array">
                <label>Encoder-Server URL</label>
                <config>
                    <type>input</type>
                    <size>50</size>
                    <eval>required,trim</eval>
                    <default>localhost</default>
                </config>
            </TCEforms>
        </sensorhost>
        <poiicon>
            <TCEforms type="array">
                <label>POI Icon</label>
                <config type="array">
                    <type>group</type>    
                    <internal_type>file</internal_type>
                    <allowed>png</allowed>                   
                    <max_size>2000</max_size>
                    <show_thumbs>1</show_thumbs>
                    <size>3</size>
                    <minitems>0</minitems>
                    <maxitems>1</maxitems>
                </config>
            </TCEforms>
        </poiicon>
        <langicon_impress>
            <TCEforms type="array">
                <label>Icon Impressum</label>
                <config type="array">
                    <type>group</type>    
                    <internal_type>file</internal_type>
                    <allowed>png</allowed>                   
                    <max_size>2000</max_size>
                    <show_thumbs>1</show_thumbs>
                    <size>3</size>
                    <minitems>0</minitems>
                    <maxitems>1</maxitems>
                </config>
            </TCEforms>
        </langicon_impress>
        <langicon_de>
            <TCEforms type="array">
                <label>Icon Deutsch</label>
                <config type="array">
                    <type>group</type>    
                    <internal_type>file</internal_type>
                    <allowed>png</allowed>                   
                    <max_size>2000</max_size>
                    <show_thumbs>1</show_thumbs>
                    <size>3</size>
                    <minitems>0</minitems>
                    <maxitems>1</maxitems>
                </config>
            </TCEforms>
        </langicon_de>
        <langicon_en>
            <TCEforms type="array">
                <label>Icon Englisch</label>
                <config type="array">
                    <type>group</type>    
                    <internal_type>file</internal_type>
                    <allowed>png</allowed>                   
                    <max_size>2000</max_size>
                    <show_thumbs>1</show_thumbs>
                    <size>3</size>
                    <minitems>0</minitems>
                    <maxitems>1</maxitems>
                </config>
            </TCEforms>
        </langicon_en>
        <specialIconSize>
            <TCEforms type="array">
                <label>Spezial POI - Größe (px)</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <eval>int,required,trim</eval>
                    <range>
                      <lower>0</lower>
                      <higher>300</higher>
                    </range>
                    <default>150</default>
                </config>
            </TCEforms>
        </specialIconSize>
        <debugmode>
            <TCEforms type="array">
                <label>Debugmodus aktivieren</label>
                <config>
                    <type>check</type>
                    <default>0</default>
                </config>
            </TCEforms>
        </debugmode>
        <!-- kuratormode>
            <TCEforms type="array">
                <label>Kuratormodus aktivieren</label>
                <config>
                    <type>check</type>
                    <default>0</default>
                </config>
            </TCEforms>
        </kuratormode -->
    </el>
</ROOT>
</T3DataStructure>
